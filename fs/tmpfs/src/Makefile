obj-$(CONFIG_BISCUITOS_TMPFS_FS)	+= tmpfs.o

# Tmpfs configure
ccflags-$(CONFIG_BISCUITOS_TMPFS_FS) += -DCONFIG_BISCUITOS_TMPFS_BLOCKS=0x1000
ccflags-$(CONFIG_BISCUITOS_TMPFS_FS) += -DCONFIG_BISCUITOS_TMPFS_INODES=0x1000

tmpfs-$(CONFIG_BISCUITOS_TMPFS_FS)	+= shmem.o
tmpfs-$(CONFIG_BISCUITOS_TMPFS_FS)	+= inode.o
